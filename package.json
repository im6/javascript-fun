{
  "name": "javascript-fun",
  "version": "1.7.0",
  "description": "source code generator for javascript.fun",
  "engines": {
    "node": ">=12.14.1"
  },
  "scripts": {
    "daily": "yarn collect && yarn render && yarn cp",
    "lint": "eslint ./webpack ./src",
    "test": "jest --coverage",
    "dev": "webpack --config ./webpack/develop.js",
    "build": "webpack --config ./webpack/production.js",
    "build-misc": "webpack --config ./webpack/miscellaneous.js",
    "web": "node ./dist/node/server.js",
    "render": "node ./dist/node/render.js",
    "admin": "node -r dotenv/config dist/node/admin.js dotenv_config_path=../javascript-fun.env",
    "collect": "node -r dotenv/config dist/node/collect.js dotenv_config_path=../javascript-fun.env",
    "cp": ". ./cp.sh"
  },
  "repository": "git+https://github.com/im6/javascript-fun.git",
  "author": "im6",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/im6/javascript-fun/issues"
  },
  "homepage": "https://github.com/im6/javascript-fun#readme",
  "dependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.12.13",
    "@testing-library/react": "^11.2.5",
    "async": "^3.2.0",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "cheerio": "^1.0.0-rc.5",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "lodash.groupby": "^4.6.0",
    "lodash.orderby": "^4.6.0",
    "mini-css-extract-plugin": "^1.3.9",
    "mysql": "^2.18.1",
    "node-fetch": "^2.6.1",
    "numeral": "^2.0.6",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.2.8",
    "postcss-loader": "^5.2.0",
    "prettier": "^2.2.1",
    "progress": "^2.0.3",
    "prompts": "^2.4.0",
    "purecss": "^2.0.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typed.js": "^2.0.11",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^2.5.2"
  },
  "browserslist": [
    "defaults",
    "cover 99.5%"
  ],
  "prettier": {
    "endOfLine": "auto",
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "prettier"
    ],
    "plugins": [
      "react",
      "jsx-a11y",
      "import",
      "prettier",
      "jest"
    ],
    "env": {
      "browser": true,
      "jest/globals": true
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ]
  },
  "jest": {
    "verbose": false,
    "clearMocks": true,
    "moduleDirectories": [
      "node_modules"
    ],
    "setupFiles": [
      "<rootDir>/src/testing/setEnvVars.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/src/testing/styleMock.js"
    },
    "coverageDirectory": "coverage",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": -10
      }
    }
  }
}
