{
  "name": "javascript-fun",
  "version": "1.12.0",
  "description": "source code generator for javascript.fun",
  "engines": {
    "node": ">=18.16.0"
  },
  "repository": "git+https://github.com/im6/javascript-fun.git",
  "author": "im6",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/im6/javascript-fun/issues"
  },
  "homepage": "https://github.com/im6/javascript-fun#readme",
  "dependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@testing-library/react": "^14.0.0",
    "@types/express": "^4.17.17",
    "@types/numeral": "^2.0.2",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "app-constant": "1.0.0",
    "autoprefixer": "^10.4.14",
    "classnames": "^2.3.1",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "express": "^4.18.1",
    "jest-environment-jsdom": "^29.5.0",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "mini-css-extract-plugin": "^2.7.5",
    "numeral": "^2.0.6",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.2.4",
    "purecss": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-loader": "^9.3.1",
    "typed.js": "^2.0.15",
    "webpack": "^5.79.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "browserslist": [
    "defaults",
    "cover 99.5%"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ]
  },
  "jest": {
    "verbose": false,
    "testEnvironment": "jsdom",
    "clearMocks": true,
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/src/testing/styleMock.js"
    },
    "coverageDirectory": "coverage",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": -10
      }
    }
  }
}
