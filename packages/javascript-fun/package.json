{
  "name": "javascript-fun",
  "version": "1.11.0",
  "description": "source code generator for javascript.fun",
  "engines": {
    "node": ">=16.13.0"
  },
  "repository": "git+https://github.com/im6/javascript-fun.git",
  "author": "im6",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/im6/javascript-fun/issues"
  },
  "homepage": "https://github.com/im6/javascript-fun#readme",
  "dependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@testing-library/react": "^13.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/numeral": "^2.0.2",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "app-constant": "1.0.0",
    "autoprefixer": "^10.4.4",
    "classnames": "^2.3.1",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "express": "^4.17.3",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "mini-css-extract-plugin": "^2.6.0",
    "numeral": "^2.0.6",
    "postcss": "^8.4.12",
    "postcss-loader": "^6.2.0",
    "purecss": "^2.1.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "ts-loader": "^9.2.8",
    "typed.js": "^2.0.12",
    "typescript": "^4.6.3",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  },
  "browserslist": [
    "defaults",
    "cover 99.5%"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ]
  },
  "jest": {
    "verbose": false,
    "testEnvironment": "jsdom",
    "clearMocks": true,
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/src/testing/styleMock.js"
    },
    "coverageDirectory": "coverage",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": -10
      }
    }
  }
}
